plugins {
    id 'org.jetbrains.kotlin.jvm'
    id 'maven-publish'
}

dependencies {
    implementation gradleApi()
}

publishing{
    publications {
        AsmPlugin(MavenPublication) {
            groupId 'org.calf.asm'
            artifactId 'plugin'
            version "1.0.0"
            from components.java
        }

    }
}